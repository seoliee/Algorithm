package SWEA.부분수열의합2817;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Solution2 {
	
	public static void main(String[] args) throws FileNotFoundException {
		
		File file = new File("./src/SWEA/부분수열의합2917/input.txt");
		Scanner sc = new Scanner(file);
		
		// 테스트케이스
		int T = sc.nextInt();
		
		// 테스트케이스만큼 반복
		for(int tc = 1; tc <= T; tc++) {
			int N = sc.nextInt();
			int K = sc.nextInt();
			
			int[] A = new int[N];
			
			for(int i = 0; i < N; i++) {
				A[i] = sc.nextInt();
			}
			
			int cnt = 0;
			
			for(int i = 0; i < (1<<N); i++) {  // 조합의 경우의 수
				int sum = 0;
				for(int j = 0; j < i; j++) {
					if((i & (1<<j)) != 0) {
						sum += A[i];
					}
				}
				if(sum == K) {
					cnt ++;
				}
			}
			
			System.out.println("#" + tc + " " + cnt);
			
			
			
		}  // 테스트케이스만큼 반복 끝
	}
}
