package 백준.단어의개수1152;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
//		String inp = sc.next();
		
		int result = 0;
		// 계속 입력 받다가 줄바꿈??나오면 그만 받기 해야 할거같은데
		while(sc.next() != "\n") {
			result++;
		}
		
		System.out.println(result);
		
	}
}
