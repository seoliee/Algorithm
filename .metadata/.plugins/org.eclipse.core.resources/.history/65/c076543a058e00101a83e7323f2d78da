package 백준.문자열9086;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String inp = sc.next();
		int len = inp.length();
		
		char first = inp.charAt(0);
		char last = inp.charAt(len - 1);
		
		System.out.println(first + last);
	}
}
