package 백준.다리놓기_1010;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// 테스트케이스
		int T = sc.nextInt();
		
		// 테스트케이스만큼 반복 시작
		for(int tc = 1; tc <= T; tc++) {
			int N = sc.nextInt();
			int M = sc.nextInt();

			int top = 1;
			int bottom = 1;
			
			for(int i = M; i >= N; i--) {
				top *= i;
			}
			
			for(int i = N; i > 0 ; i--) {
				bottom *= i;
			}
			
			int result = top / bottom;
			
			System.out.println(result);
			
		}  // 테스트케이스만큼 반복 끝
	}

}
